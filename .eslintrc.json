{
  "extends": "airbnb-base",
  "plugins": [
      "import"
  ],
  "rules": {
    "radix": [0],
    "arrow-body-style": [0]
  },
  "env": {
    "browser": true,
    "node": true,
    "mocha": true
  },
  "globals": {
    "document": false,
    "describe": false,
    "it": false,
    "beforeEach": false,
    "should": false
  }
}
